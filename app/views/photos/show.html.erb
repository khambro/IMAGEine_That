<div class= "container">

  <%= link_to "Upload New Photo", "/new-photo" %>
  <%= button_to "Subscribe", send_subscribe_path, method: :post %>
  <h2>What splendid photos, <%= current_user.email %>!</h2>
  <% @photos.each do |p| %>
    <% if p.user_id == current_user.id %>
      <div class= "box">
        <div class="photo">
          <%= link_to image_tag(p.image_file.url(:square)), "/photo/#{p.id}"  %>
        </div>  
        <div class="description">
          <%= p.description %>
        </div>
      </div>
    <% end %>
  <% end %>

</div>
